<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">name</th>
      <th scope="col">location</th>
      <th scope="col">house</th>
    </tr>
  </thead>
  <tbody>
<% @Character.each do |c| %>
    <tr>
      <th scope="row"><%="#{c.id}"%></th>
      <td><%= "#{c.name}"%></td>
      <td><%= "#{c.location}"%></td>
      <td><%= "#{c.house}"%></td>
    </tr>
    <% end%>
    </tbody>
    </table>